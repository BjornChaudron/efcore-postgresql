services:
  efcore-postgresql-web-api:
    build:
      args:
        source: obj/Docker/empty/
      context: C:\Users\Bjorn\Documents\Visual Studio 2017\Projects\efcore-postgresql\efcore-postgresql-web-api
      dockerfile: Dockerfile
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      BLOG_DB_CONNECTION_STRING: Host=postgres;Database=BlogDb;Username=root;Password=root
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
    image: efcore-postgresql-web-api:dev
    labels:
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - 8081:80/tcp
    - 80/tcp
    volumes:
    - C:\Users\Bjorn\Documents\Visual Studio 2017\Projects\efcore-postgresql\efcore-postgresql-web-api:/app:rw
    - C:\Users\Bjorn\vsdbg:/remote_debugger:ro
  postgres:
    environment:
      POSTGRES_DB: BlogDb
      POSTGRES_PASSWORD: root
      POSTGRES_USER: root
    image: postgres
    ports:
    - 5432:5432/tcp
    restart: always
version: '3.0'